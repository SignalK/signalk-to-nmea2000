{
  "name": "signalk-to-nmea2000",
  "version": "2.21.0",
  "description": "Signal K server plugin to convert Signal K to NMEA2000",
  "main": "./dist/index.js",
  "scripts": {
    "test": "mocha --timeout 10000",
    "create-release": "github-create-release --owner signalk --repository signalk-to-nmea2000",
    "release": "git tag -d v$npm_package_version ; git tag v$npm_package_version && git push --tags && git push && npm run create-release",
    "build": "tsc -b --pretty false",
    "watch": "tsc --watch --pretty false"
  },
  "keywords": [
    "signalk-node-server-plugin"
  ],
  "author": {
    "name": "Scott Bender",
    "email": "scott@scottbender.net"
  },
  "contributors": [
    {
      "name": "Teppo Kurki"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "@canboat/ts-pgns": "^1.11.3",
    "@signalk/server-api": "^2.7.1",
    "baconjs": "^1.0.1",
    "lodash": "^4.17.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SignalK/signalk-to-nmea2000"
  },
  "devDependencies": {
    "@canboat/canboatjs": "^3.0.2",
    "@signalk/github-create-release": "^1.2.1",
    "@tsconfig/node20": "^20.1.6",
    "@types/baconjs": "^0.7.34",
    "chai": "^4.1.2",
    "chai-json-equal": "^0.0.1",
    "mocha": "^5.0.0",
    "signalk-server": "^1.4.0",
    "sinon": "^4.1.6",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=10"
  }
}
